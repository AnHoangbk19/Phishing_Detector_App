{
    "name": "Get URL",
    "version": "1.0",
    "description": "Lấy địa chỉ URL",
    "action":{
      "default_popup": "plugin_ui.html"
    },
    "content_scripts":[
        {
          "matches": ["http://*/*","https://*/*"],
          "css": ["css/style.css"],
          "js": ["js/main.js"]
        }
      ],
      "manifest_version": 3
  }